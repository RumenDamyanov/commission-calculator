# Application
APP_ENV=dev
APP_DEBUG=true

# BIN Lookup Service
BIN_LOOKUP_URL=https://lookup.binlist.net
BIN_LOOKUP_API_KEY=your_bin_lookup_api_key_here
BIN_LOOKUP_RATE_LIMIT=100
BIN_LOOKUP_CACHE_TTL=3600

# Exchange Rates Service
EXCHANGE_RATES_URL=https://api.apilayer.com/exchangerates_data/latest
EXCHANGE_RATES_API_KEY=your_exchange_rates_api_key_here
EXCHANGE_RATES_RATE_LIMIT=30
EXCHANGE_RATES_CACHE_TTL=3600

# Commission Rates
EU_COMMISSION_RATE=0.01
NON_EU_COMMISSION_RATE=0.02

# Cache settings
CACHE_ENABLED=true
CACHE_NAMESPACE=commission_calculator
DEFAULT_CACHE_TTL=3600

# Rate Limiting
RATE_LIMIT_ENABLED=true

# Logging
LOG_CHANNEL=commission_calculator
LOG_LEVEL=info

# Additional timeout settings
BIN_LOOKUP_TIMEOUT=5
EXCHANGE_RATES_TIMEOUT=5
